@prefix : <http://www.smolang.org/greenhouseDT#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.smolang.org/greenhouseDT#> .

<http://www.smolang.org/greenhouseDT#> rdf:type owl:Ontology .

#################################################################
#    Datatypes
#################################################################

###  http://www.smolang.org/greenhouseDT#percentage
:percentage rdf:type rdfs:Datatype ;
            owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                  owl:onDatatype xsd:double ;
                                  owl:withRestrictions ( [ xsd:minInclusive "0.0"^^xsd:double
                                                         ]
                                                         [ xsd:maxInclusive "100.0"^^xsd:double
                                                         ]
                                                       )
                                ] .


#################################################################
#    Object Properties
#################################################################

###  http://www.smolang.org/greenhouseDT#hasHealthState
:hasHealthState rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Plant ;
                rdfs:range :HealthState .


###  http://www.smolang.org/greenhouseDT#hasPlant
:hasPlant rdf:type owl:ObjectProperty ;
          rdfs:domain :Pot ;
          rdfs:range :Plant .


###  http://www.smolang.org/greenhouseDT#hasPot
:hasPot rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:domain :Shelf ;
        rdfs:range :Pot .


###  http://www.smolang.org/greenhouseDT#hasShelf
:hasShelf rdf:type owl:ObjectProperty ;
          rdfs:domain :Greenhouse ;
          rdfs:range :Shelf .


###  http://www.smolang.org/greenhouseDT#hasWaterTank
:hasWaterTank rdf:type owl:ObjectProperty ;
              rdfs:domain :Greenhouse ;
              rdfs:range :WaterTank .


###  http://www.smolang.org/greenhouseDT#wateredBy
:wateredBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Pot ;
           rdfs:range :Pump .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty owl:inverseOf owl:topObjectProperty ;
                      rdf:type owl:SymmetricProperty ,
                               owl:TransitiveProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.smolang.org/greenhouseDT#hasGroupPosition
:hasGroupPosition rdf:type owl:DatatypeProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Pot
                                              :Pump
                                            )
                              ] ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:oneOf [ rdf:type rdf:List ;
                                           rdf:first "left" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "right" ;
                                                      rdf:rest rdf:nil
                                                    ]
                                         ]
                             ] .


###  http://www.smolang.org/greenhouseDT#hasHumidity
:hasHumidity rdf:type owl:DatatypeProperty ;
             rdfs:domain :Shelf ;
             rdfs:range :percentage .


###  http://www.smolang.org/greenhouseDT#hasHumidityGpioPin
:hasHumidityGpioPin rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Shelf ;
                    rdfs:range xsd:int .


###  http://www.smolang.org/greenhouseDT#hasIdealTemperature
:hasIdealTemperature rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Plant ;
                     rdfs:range xsd:double .


###  http://www.smolang.org/greenhouseDT#hasLightStrength
:hasLightStrength rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Greenhouse ;
                  rdfs:range :percentage .


###  http://www.smolang.org/greenhouseDT#hasMoisture
:hasMoisture rdf:type owl:DatatypeProperty ;
             rdfs:domain :Pot ;
             rdfs:range :percentage .


###  http://www.smolang.org/greenhouseDT#hasMoistureAdcChannel
:hasMoistureAdcChannel rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:domain :Pot ;
                       rdfs:range xsd:int .


###  http://www.smolang.org/greenhouseDT#hasTemperature
:hasTemperature rdf:type owl:DatatypeProperty ;
                rdfs:domain :Shelf ;
                rdfs:range xsd:double .


###  http://www.smolang.org/greenhouseDT#hasTemperatureGpioPin
:hasTemperatureGpioPin rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Shelf ;
                       rdfs:range xsd:int .


###  http://www.smolang.org/greenhouseDT#hasWaterFlow
:hasWaterFlow rdf:type owl:DatatypeProperty ;
              rdfs:domain :Pump .


###  http://www.smolang.org/greenhouseDT#hasWaterLevel
:hasWaterLevel rdf:type owl:DatatypeProperty ;
               rdfs:domain :WaterTank ;
               rdfs:range :percentage .


###  http://www.smolang.org/greenhouseDT#idealMoisture
:idealMoisture rdf:type owl:DatatypeProperty ;
               rdfs:domain :Plant .


###  http://www.smolang.org/greenhouseDT#maxNdvi
:maxNdvi rdf:type owl:DatatypeProperty ;
         rdfs:domain :HealthState .


###  http://www.smolang.org/greenhouseDT#minNdvi
:minNdvi rdf:type owl:DatatypeProperty ;
         rdfs:domain :HealthState .


###  http://www.smolang.org/greenhouseDT#modelName
:modelName rdf:type owl:DatatypeProperty ;
           rdfs:domain :Pump ;
           rdfs:range xsd:string .


###  http://www.smolang.org/greenhouseDT#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain owl:Thing .


###  http://www.smolang.org/greenhouseDT#ndvi
:ndvi rdf:type owl:DatatypeProperty ;
      rdfs:domain :Plant ;
      rdfs:range xsd:double .


###  http://www.smolang.org/greenhouseDT#plantId
:plantId rdf:type owl:DatatypeProperty ;
         rdfs:domain :Plant ;
         rdfs:range xsd:string .


###  http://www.smolang.org/greenhouseDT#potPosition
:potPosition rdf:type owl:DatatypeProperty ;
             rdfs:domain :Pot ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:oneOf [ rdf:type rdf:List ;
                                      rdf:first "left" ;
                                      rdf:rest [ rdf:type rdf:List ;
                                                 rdf:first "right" ;
                                                 rdf:rest rdf:nil
                                               ]
                                    ]
                        ] .


###  http://www.smolang.org/greenhouseDT#powerOutput
:powerOutput rdf:type owl:DatatypeProperty ;
             rdfs:domain :Pump ;
             rdfs:range xsd:integer .


###  http://www.smolang.org/greenhouseDT#pumpGpioPin
:pumpGpioPin rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Pump ;
             rdfs:range xsd:int .


###  http://www.smolang.org/greenhouseDT#pumpId
:pumpId rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty .


###  http://www.smolang.org/greenhouseDT#pumpLifeTime
:pumpLifeTime rdf:type owl:DatatypeProperty ;
              rdfs:domain :Pump ;
              rdfs:range xsd:integer .


###  http://www.smolang.org/greenhouseDT#shelfFloor
:shelfFloor rdf:type owl:DatatypeProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Pot
                                        :Pump
                                        :Shelf
                                      )
                        ] ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
                                     rdf:first "1" ;
                                     rdf:rest [ rdf:type rdf:List ;
                                                rdf:first "2" ;
                                                rdf:rest rdf:nil
                                              ]
                                   ]
                       ] .


###  http://www.smolang.org/greenhouseDT#temperature
:temperature rdf:type owl:DatatypeProperty ;
             rdfs:domain :Pump ;
             rdfs:range xsd:double .


#################################################################
#    Classes
#################################################################

###  http://www.smolang.org/greenhouseDT#BadHealthState
:BadHealthState rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                             owl:onProperty :ndvi ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:double ;
                                                                                  owl:withRestrictions ( [ xsd:minInclusive "-0.33"^^xsd:double
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :ndvi ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:double ;
                                                                                  owl:withRestrictions ( [ xsd:maxInclusive "0.32"^^xsd:double
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :HealthState ,
                                :Plant ;
                owl:disjointWith :DeadHealthState ,
                                 :GoodHealthState .


###  http://www.smolang.org/greenhouseDT#Basil
:Basil rdf:type owl:Class ;
       rdfs:subClassOf :Plant ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :idealMoisture ;
                         owl:hasValue "50.0"^^xsd:double
                       ] .


###  http://www.smolang.org/greenhouseDT#DeadHealthState
:DeadHealthState rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                              owl:onProperty :ndvi ;
                                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                   owl:onDatatype xsd:double ;
                                                                                   owl:withRestrictions ( [ xsd:minInclusive "-1.0"^^xsd:double
                                                                                                          ]
                                                                                                        )
                                                                                 ]
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :ndvi ;
                                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                   owl:onDatatype xsd:double ;
                                                                                   owl:withRestrictions ( [ xsd:maxInclusive "-0.34"^^xsd:double
                                                                                                          ]
                                                                                                        )
                                                                                 ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :HealthState ,
                                 :Plant ;
                 owl:disjointWith :GoodHealthState .


###  http://www.smolang.org/greenhouseDT#GoodHealthState
:GoodHealthState rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                              owl:onProperty :ndvi ;
                                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                   owl:onDatatype xsd:double ;
                                                                                   owl:withRestrictions ( [ xsd:minInclusive "0.33"^^xsd:double
                                                                                                          ]
                                                                                                        )
                                                                                 ]
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :ndvi ;
                                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                   owl:onDatatype xsd:double ;
                                                                                   owl:withRestrictions ( [ xsd:maxInclusive "1.0"^^xsd:double
                                                                                                          ]
                                                                                                        )
                                                                                 ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :HealthState ,
                                 :Plant .


###  http://www.smolang.org/greenhouseDT#Greenhouse
:Greenhouse rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasWaterTank ;
                              owl:someValuesFrom :WaterTank
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasShelf ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Shelf
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasLightStrength ;
                              owl:someValuesFrom :percentage
                            ] .


###  http://www.smolang.org/greenhouseDT#HealthState
:HealthState rdf:type owl:Class .


