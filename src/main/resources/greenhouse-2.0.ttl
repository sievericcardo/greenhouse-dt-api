<http://www.smolang.org/greenhouseDT> rdf:type owl:Ontology .

#################################################################
#    Datatypes
#################################################################

###  http://www.smolang.org/greenhouseDT#percentage
ast:percentage rdf:type rdfs:Datatype ;
               owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                     owl:onDatatype xsd:double ;
                                     owl:withRestrictions ( [ xsd:minInclusive "0.0"^^xsd:double
                                                            ]
                                                            [ xsd:maxInclusive "100.0"^^xsd:double
                                                            ]
                                                          )
                                   ] .


#################################################################
#    Object Properties
#################################################################

###  http://www.smolang.org/greenhouseDT#containsPot
ast:containsPot rdf:type owl:ObjectProperty ;
                rdfs:domain ast:Section ;
                rdfs:range ast:Pot .


###  http://www.smolang.org/greenhouseDT#containsWaterBucket
ast:containsWaterBucket rdf:type owl:ObjectProperty ;
                        rdfs:domain ast:Greenhouse ;
                        rdfs:range ast:WaterBucket .


###  http://www.smolang.org/greenhouseDT#hasLightSensor
ast:hasLightSensor rdf:type owl:ObjectProperty ;
                   rdfs:domain ast:Greenhouse ;
                   rdfs:range ast:LightSensor .


###  http://www.smolang.org/greenhouseDT#hasLightSwitch
ast:hasLightSwitch rdf:type owl:ObjectProperty ;
                   rdfs:domain ast:Greenhouse ;
                   rdfs:range ast:Light_Switch .


###  http://www.smolang.org/greenhouseDT#hasMoistureSensor
ast:hasMoistureSensor rdf:type owl:ObjectProperty ;
                      rdfs:domain ast:Pot ;
                      rdfs:range ast:MoistureSensor .


###  http://www.smolang.org/greenhouseDT#hasNutrientSensor
ast:hasNutrientSensor rdf:type owl:ObjectProperty ;
                      rdfs:domain ast:Pot ;
                      rdfs:range ast:NutrientSensor .


###  http://www.smolang.org/greenhouseDT#hasTemperatureHumiditySensor
ast:hasTemperatureHumiditySensor rdf:type owl:ObjectProperty ;
                                 rdfs:domain ast:Greenhouse ;
                                 rdfs:range ast:TemperatureHumiditySensor .


###  http://www.smolang.org/greenhouseDT#hasWaterLevelSensor
ast:hasWaterLevelSensor rdf:type owl:ObjectProperty ;
                        rdfs:domain ast:WaterBucket ;
                        rdfs:range ast:WaterLevel .


###  http://www.smolang.org/greenhouseDT#hasWaterStrategy
ast:hasWaterStrategy rdf:type owl:ObjectProperty .


###  http://www.smolang.org/greenhouseDT#isInPot
ast:isInPot rdf:type owl:ObjectProperty ;
