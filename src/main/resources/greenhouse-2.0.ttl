#################################################################
#    Classes
#################################################################

###  http://www.smolang.org/greenhouseDT#Actuator
ast:Actuator rdf:type owl:Class .


###  http://www.smolang.org/greenhouseDT#Basil
ast:Basil rdf:type owl:Class ;
          rdfs:subClassOf ast:Plant ;
          owl:disjointWith ast:Rosemary ,
                           ast:Sage .


###  http://www.smolang.org/greenhouseDT#Dead
ast:Dead rdf:type owl:Class ;
         rdfs:subClassOf ast:HealthState ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ast:healthStateName ;
                           owl:hasValue "Dead"
                         ] ;
         owl:disjointWith ast:Healthy ,
                          ast:Unhealthy .


###  http://www.smolang.org/greenhouseDT#DeadBasil
ast:DeadBasil rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                           owl:onProperty ast:ndvi ;
                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                owl:onDatatype xsd:double ;
                                                                                owl:withRestrictions ( [ xsd:maxInclusive "0.0"^^xsd:double
                                                                                                       ]
                                                                                                     )
                                                                              ]
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty ast:familyName ;
                                                           owl:hasValue "Ocimum basilicum"
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf ast:Dead .


