            rdfs:range xsd:string .


###  http://www.smolang.org/greenhouseDT#potId
ast:potId rdf:type owl:DatatypeProperty ;
          rdfs:domain ast:Pot ;
          rdfs:range xsd:string .


###  http://www.smolang.org/greenhouseDT#potPosition
ast:potPosition rdf:type owl:DatatypeProperty ;
                rdfs:domain ast:Pot ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "left" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "right" ;
                                                    rdf:rest rdf:nil
                                                  ]
                                       ]
                           ] .


###  http://www.smolang.org/greenhouseDT#powerOutput
ast:powerOutput rdf:type owl:DatatypeProperty ;
