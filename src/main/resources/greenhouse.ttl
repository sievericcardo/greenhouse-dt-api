###  http://www.smolang.org/greenhouseDT#shelfFloor
ast:shelfFloor rdf:type owl:DatatypeProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( ast:Pot
                                           ast:Pump
                                           ast:Shelf
                                         )
                           ] ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:oneOf [ rdf:type rdf:List ;
                                        rdf:first "1" ;
                                        rdf:rest [ rdf:type rdf:List ;
                                                   rdf:first "2" ;
                                                   rdf:rest rdf:nil
                                                 ]
                                      ]
                          ] .


###  http://www.smolang.org/greenhouseDT#status
ast:status rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain ast:Plant ;
           rdfs:range xsd:string .


###  http://www.smolang.org/greenhouseDT#temperature
ast:temperature rdf:type owl:DatatypeProperty ;
                rdfs:domain ast:Pump ;
                rdfs:range xsd:double .


#################################################################
#    Classes
#################################################################

###  http://www.smolang.org/greenhouseDT#BadHealthState
ast:BadHealthState rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty ast:ndvi ;
                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                     owl:onDatatype xsd:double ;
                                                                                     owl:withRestrictions ( [ xsd:minInclusive "-0.33"^^xsd:double
                                                                                                            ]
                                                                                                          )
                                                                                   ]
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty ast:ndvi ;
                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                     owl:onDatatype xsd:double ;
                                                                                     owl:withRestrictions ( [ xsd:maxInclusive "0.32"^^xsd:double
                                                                                                            ]
                                                                                                          )
                                                                                   ]
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf ast:HealthState ;
                   owl:disjointWith ast:DeadHealthState ,
                                    ast:GoodHealthState .


###  http://www.smolang.org/greenhouseDT#Basil
ast:Basil rdf:type owl:Class ;
          rdfs:subClassOf ast:Plant ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ast:idealMoisture ;
                            owl:hasValue "50.0"^^xsd:double
                          ] .


###  http://www.smolang.org/greenhouseDT#DeadHealthState
ast:DeadHealthState rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty ast:ndvi ;
                                                                 owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                      owl:onDatatype xsd:double ;
                                                                                      owl:withRestrictions ( [ xsd:minInclusive "-1.0"^^xsd:double
                                                                                                             ]
                                                                                                           )
                                                                                    ]
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty ast:ndvi ;
                                                                 owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                      owl:onDatatype xsd:double ;
                                                                                      owl:withRestrictions ( [ xsd:maxInclusive "-0.34"^^xsd:double
                                                                                                             ]
                                                                                                           )
                                                                                    ]
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf ast:HealthState ;
                    owl:disjointWith ast:GoodHealthState .


###  http://www.smolang.org/greenhouseDT#GoodHealthState
ast:GoodHealthState rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty ast:ndvi ;
                                                                 owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                      owl:onDatatype xsd:double ;
                                                                                      owl:withRestrictions ( [ xsd:minInclusive "0.33"^^xsd:double
                                                                                                             ]
                                                                                                           )
                                                                                    ]
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty ast:ndvi ;
                                                                 owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                      owl:onDatatype xsd:double ;
                                                                                      owl:withRestrictions ( [ xsd:maxInclusive "1.0"^^xsd:double
                                                                                                             ]
                                                                                                           )
                                                                                    ]
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf ast:HealthState .


###  http://www.smolang.org/greenhouseDT#Greenhouse
ast:Greenhouse rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty ast:hasWaterTank ;
                                 owl:someValuesFrom ast:WaterTank
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty ast:hasShelf ;
                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass ast:Shelf
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty ast:hasLightStrength ;
                                 owl:someValuesFrom ast:percentage
                               ] .


###  http://www.smolang.org/greenhouseDT#HealthState
ast:HealthState rdf:type owl:Class .


